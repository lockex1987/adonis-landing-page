    "watch-fe": "mix watch",
    "prod-fe": "mix --production",
    "lint-fe": "eslint --fix --ext=.js,.vue frontend/js",
    "clear-fe": "rm public/js/* & rm public/css/*"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "devDependencies": {
    "@adonisjs/assembler": "^5.6.2",
    "@japa/preset-adonis": "^1.0.15",
    "@japa/runner": "^2.0.7",
