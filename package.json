    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "adonis-preset-ts": "^2.1.0",
    "bootstrap": "^5.1.3",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-vue": "^8.6.0",
    "laravel-mix": "^6.0.43",
    "pino-pretty": "^7.6.1",
    "resolve-url-loader": "^5.0.0",
