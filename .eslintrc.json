{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "globals": {
    "Vue": "readonly",
    "Vuex": "readonly",
    "VueRouter": "readonly",
    "axios": "readonly",
    "Highcharts": "readonly",
    "$": "readonly",
    "ExcelJS": "readonly",
    "moment": "readonly",
    "FB": "readonly",
    "YT": "readonly",
    "L": "readonly",
    "bootstrap": "readonly",
    "CV": "readonly",
    "noti": "readonly",
    "Datatable": "readonly",
    "Pagi": "readonly",
    "CommonUtils": "readonly",
    "Inputmask": "readonly",
    "Carousel": "readonly",
    "THREE": "readonly",
    "SSO_CONSUMER_DOMAIN": "readonly",
    "SSO_PASSPORT_URL": "readonly"
  },
  "extends": [
    "google",
    "plugin:@typescript-eslint/recommended",
    "plugin:vue/essential"
  ],
  "rules": {
    "require-jsdoc": "off",
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "indent": [
      "error",
      2
    ],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "eqeqeq": "off",
    "no-new": "off",
    "no-multiple-empty-lines": "off",
    "operator-linebreak": [
      "error",
      "before"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "@typescript-eslint/indent": [
      "error",
      2
    ],
    "max-len": "off",
    "quote-props": [
      "error",
      "as-needed"
    ],
    "valid-jsdoc": "off",
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "vue/require-v-for-key": "off",
    "vue/require-valid-default-prop": "off",
    "vue/no-multiple-template-root": "off",
    "vue/no-v-model-argument": "off"
  }
}
